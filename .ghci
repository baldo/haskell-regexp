-- generate parser from happy specification on GHCi startup

:def genparser \_ -> return ":! happy Text/RegExp/Parser.y"
:genparser

-- enable overloaded strings

:set -XOverloadedStrings

-- load private modules

:def loadprivate \_ -> return ":load Text/RegExp Text/RegExp/Data Text/RegExp/Matcher Data/Semiring"
:loadprivate
